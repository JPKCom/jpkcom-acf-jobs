<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
    configVersion="3"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://www.phpdoc.org"
    xsi:noNamespaceSchemaLocation="https://docs.phpdoc.org/latest/phpdoc.xsd"
>
    <title>JPKCom ACF Jobs - API Documentation</title>
    <paths>
        <output>/home/jpk/wp/jpkcom-acf-jobs/docs</output>
    </paths>
    <version number="1.0.0">
        <folder>/home/jpk/wp/jpkcom-acf-jobs/docs</folder>
        <api>
            <source dsn="/home/jpk/wp/jpkcom-acf-jobs">
                <path>jpkcom-acf-jobs.php</path>
                <path>includes</path>
            </source>
            <output>/home/jpk/wp/jpkcom-acf-jobs/docs</output>
            <ignore hidden="true" symlinks="true">
                <path>vendor/*</path>
                <path>templates/*</path>
                <path>debug-templates/*</path>
                <path>languages/*</path>
                <path>assets/*</path>
                <path>.git/*</path>
                <path>.github/*</path>
                <path>.claude/*</path>
                <path>.ht.acf-json/*</path>
                <path>*.py</path>
                <path>*.md</path>
                <path>*.xml</path>
                <path>*.mo</path>
                <path>*.po</path>
                <path>*.pot</path>
            </ignore>
            <default-package-name>JPKComAcfJobs</default-package-name>
        </api>
    </version>
    <template name="default"/>
    <setting name="graphs.enabled" value="true"/>
    <setting name="graphs.format" value="svg"/>
</phpdocumentor>
